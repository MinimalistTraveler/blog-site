{
  "name": "blog-site-server",
  "version": "1.0.0",
  "description": "This is the server for a sample blog site I have created.",
  "main": "server.js",
  "author": "Erik Bridges",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run start --prefix client\"",
    "server": "node server",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "test": "jest --watchAll --detectOpenHandles --forceExit"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-promise-router": "^3.0.3",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.15.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.5.0"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "jest-tobetype": "^1.2.0",
    "supertest": "^3.3.0",
    "winston": "^3.1.0"
  }
}
